{
  "properties": {
    "displayName": "Deny Non-West Europe Locations",
    "description": "This policy denies resource creation outside westeurope.",
    "mode": "All",
    "policyType": "Custom",
    "parameters": {},
    "policyRule": {
      "if": {
        "field": "location",
        "notIn": [
          "westeurope"
        ]
      },
      "then": {
        "effect": "deny"
      }
    }
  }
}
